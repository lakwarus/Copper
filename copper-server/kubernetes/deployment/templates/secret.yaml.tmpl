apiVersion: v1
kind: Secret
metadata:
    name: email-secret
    namespace: copper
type: Opaque
stringData:
    TELEGRAF_VERSION: 1.8.1-1
    MYSQL_HOST: mysql
    MYSQL_DATABASE: <<DATABASE>>
    MYSQL_USER: <<MYSQL-USER>>
    MYSQL_PASSWORD: <<MYSQL-PASSWD>>
    CN: <<CN-ADMIN>>
    DC1: <<DC1>>
    DC2: <<DC2>>
    DC3: <<DC3>>
    DNPASS: <<CN-ADMIN-PASSWD>>
    RO: <<RO>>
    ROPASS: <<RO-PASSWD>>
    PHPLDAPADMIN_LDAP_HOSTS: ldap
    PHPLDAPADMIN_SERVER_ADMIN: <<CN-ADMIN>>@<<DOMAIN>>
    PHPLDAPADMIN_SERVER_PATH: /phpldapadmin
    PHPLDAPADMIN_HTTPS: "false"
    PHPLDAPADMIN_HTTPS_CRT_FILENAME: cert.crt
    PHPLDAPADMIN_HTTPS_KEY_FILENAME: cert.key
    PHPLDAPADMIN_HTTPS_CA_CRT_FILENAME: ca.crt
    PHPLDAPADMIN_LDAP_CLIENT_TLS: "true"
    PHPLDAPADMIN_LDAP_CLIENT_TLS_REQCERT: try
    PHPLDAPADMIN_LDAP_CLIENT_TLS_CRT_FILENAME: cert.crt
    PHPLDAPADMIN_LDAP_CLIENT_TLS_KEY_FILENAME: cert.key
    PHPLDAPADMIN_LDAP_CLIENT_TLS_CA_CRT_FILENAME: ca.crt
    LDAP_ORGANISATION: org
    ORGNIZATION: org
    LDAP_DOMAIN: <<DOMAIN>>
    LDAP_ADMIN_PASSWORD: <<CN-ADMIN-PASSWD>>
    LDAP_LOG_LEVEL: "-1"
    LDAP_CONFIG_PASSWORD: <<CN-ADMIN-PASSWD>>
    LDAP_READONLY_USER: "true"
    LDAP_READONLY_USER_USERNAME: <<RO>>
    LDAP_READONLY_USER_PASSWORD: <<RO-PASSWD>>
    LDAP_RFC2307BIS_SCHEMA: "false"
    LDAP_BACKEND: mdb
    LDAP_TLS: "true"
    LDAP_TLS_CRT_FILENAME: cert.pem
    LDAP_TLS_KEY_FILENAME: privkey.pem
    LDAP_TLS_CA_CRT_FILENAME: fullchain.pem
    LDAP_TLS_ENFORCE: "false"
    LDAP_TLS_CIPHER_SUITE: SECURE256:+SECURE128:-VERS-TLS-ALL:+VERS-TLS1.2:-RSA:-DHE-DSS:-CAMELLIA-128-CBC:-CAMELLIA-256-CBC
    LDAP_TLS_VERIFY_CLIENT: allow
    LDAP_REPLICATION: "false"
    KEEP_EXISTING_CONFIG: "false"
    LDAP_REMOVE_CONFIG_AFTER_SETUP: "true"
    LDAP_SSL_HELPER_PREFIX: ldap
    OU : Users
    LDAP_HOST_IP : ldap
    KEY_PATH : KEYPATH
    EMAIL : <<CN-ADMIN>>@<<DOMAIN>>
    HOSTNAME : mail.<<DOMAIN>>
    FQDN : mail.test.gagana.me
    DOMAIN : <<DOMAIN>>
    REDIS_HOST : REDIS_HOST
    REDIS_PORT : REDIS_PORT
    DEBUG : "true"
    RSPAMD_PASSWORD : <<RSPAMD-PASSWD>>
